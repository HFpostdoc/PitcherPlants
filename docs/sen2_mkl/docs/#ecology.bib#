@Article{Ludwig1985,
  Title                    = {Are age-structured models appropriate for catch-effort data?},
  Author                   = {Ludwig, D. and Walters, C.J.},
  Journal                  = {Can.J.Fish.Aquat.Sci.},
  Pages                    = {1066--1072},
  Volume                   = {42},
  Year                     = {1985},

  Comment                  = {This is a gem of a paper. With elegant, well-explained equations and prose, the authors demonstrate that building more complex models (i.e. with age-structure) will decrease the usefulness of the model when the additional parameters are difficult to estimate. That is, if you can't get the information about a particular parameter (with some certitude), you might as well leave it out of the model. When the new terms have to be guesstimated, the magnitude of the error terms in the predictions (the level of uncertainty) increases dramatically. The main message is model what you've got, not what you'd like to have.},
  Keywords                 = {models, FUNCTIONAL GROUPS},
  Owner                    = {aellison},
  Refid                    = {1323},
  Timestamp                = {2013.11.20}
}

