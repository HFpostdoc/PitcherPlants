@Article{Smith1997a,
  Title                    = {Path analysis: A critical evaluation using long-term experimental data},
  Author                   = {Smith, F.A. and Brown, J.H. and Valone, T.J.},
  Journal                  = {American Naturalist},
  Pages                    = {29--42},
  Volume                   = {149},
  Year                     = {1997},
  Number                   = {1},

  Comment                  = {ISI DATA WB901 Smith FA, Univ New Mexico, Dept Biol, Albuquerque,NM 87131 USA English Article We evaluated the ability of path analysis to characterize direct and indirect interactions in a natural ecological system. Based on previous experimental work, we constructed a path diagram reflecting presumed relationships between kangaroo rats (Dipodomys spp.) and two other small rodent species. The technique was applied to examine the direct effects of competition and indirect effects mediated through vegetation. Path analysis gave varying and sometimes uninterpretable results when applied to data collected from unmanipulated and manipulated systems. The variation most likely can be attributed to some combination of inadequate specification of the path diagram, differences between unmanipulated and manipulated systems, varying responses to experiments of different duration, and natural temporal variation. Because these issues are applicable to many kinds of complex systems, we suggest caution before applying and when interpreting the results of path analysis},
  Keywords                 = {PATH ANALYSIS, COMPETITION, indirect effects, REVIEW},
  Owner                    = {aellison},
