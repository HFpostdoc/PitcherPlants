@Article{Grzebyk1994,
  Title                    = {Ecology of ciguateric dinoflagellates in the coral reef complex of Mayotte Island (SW Indian Ocean)},
  Author                   = {Grzebyk, D. and Berland, B. and Thomassin, B.A. and Bosi, C. and Arnoux, A.},
  Journal                  = {Journal of Experimental Marine Biology and Ecology},
  Pages                    = {51--66},
  Volume                   = {178},
  Year                     = {1994},

  Comment                  = {AB- The spatial distribution of ciguateric dinoflagellates was investigated in the coral reef lagoon of the island of Mayotte at the end of the dry season of 1992. Both oceanic and lagoonal waters were oligotrophic. Different substrates were investigated (algae, algal turfs and dead corals) in two areas. One area consisted of a transect between an alluvial coastal bay and the outer barrier reef, through lagoonal patch-reefs, and a second area comprised a lagoonal complex. On nearly all the substrates, the dinoflagellates Gambierdiscus toxicus Adachi & Fukuyo, Ostreopsis spp. and Prorocentrum spp., generally exhibited the following relative abundances: Ostreopsis spp. > Prorocentrum spp. > G. toxicus. Highest abundances of all species of dinoflagellates, particularly G. toxicus, occurred on dead corals, more especially on the inner slope of the barrier reef and on lagoonal patch-reefs, but not at the back of the bay nor on the barrier reef outer slope. On macroalgal substrates, dinoflagellates were moderately abundant, but some substrates, such as several red algae, supported higher abundances suggesting a stimulation. Experimental bioassays to compare growth in seawaters previously incubated with macroalgae, showed preconditioned seawaters could be either stimulatory or inhibitory. The red alga, Halymenia floresia (Cryptonemiaceae), inhibited growth of G. toxicus, while Portieria hornemanii stimulated it. Growth was also slightly stimulated by the brown algae Turbinaria ornata and Sargassum cf. turbinatifolium (Sargassaceae). Both red algae stimulated the growth of Ostreopsis ovata to a similar degree},
  Keywords                 = {dinoflagellate, coral reef, reef, ISLANDS, INDIAN OCEAN, spatial, spatial distribution, lagoons, substrate, ALGAE, MACROALGAE, Sargassum, ecology},
  Owner                    = {aellison},
  Refid                    = {6190},
  Timestamp                = {2013.11.20}
