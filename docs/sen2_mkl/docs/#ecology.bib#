@Article{Al-Mufti1977,
  Title                    = {A quantitative analysis of shoot phenology and dominance in herbaceous vegetation},
  Author                   = {Al-Mufti, M.M. and Sydes, C.L. and Furness, S.B. and Grime, J.P. and Bard, S.R.},
  Journal                  = {Journal of Ecology},
  Pages                    = {759--791},
  Volume                   = {65},
  Year                     = {1977},

  Comment                  = {This study, like Dickinson and Dodd's was conducted over a single season. These authors, however, are discussing phenology of vegetative growth, not of flowering time. They are concerned primarily with individual species within defined "communities" (areas dominated by a single species). The point of the paper is to infer competitive exclusion (based on Grimean triangles and "species characteristics" [ruderal, stress, competitive, but otherwise vague]). As far as I'm concerned, it is a big leap from observed phenological data to asserting that competition is a driving force in these communities. While I appreciate that this is one of those "classic" papers cited in most reviews of phenology, species groupings, and competition, the conclusions are only weakly supported by the data. The data are strong, but they don't convince me of anything other than the fact that different species grow at different times. Anything else must be resolved experimentally.},
  Keywords                 = {FUNCTIONAL GROUPS, PHENOLOGY},
  Owner                    = {aellison},
  Refid                    = {1319},
  Timestamp                = {2013.11.20}
}

