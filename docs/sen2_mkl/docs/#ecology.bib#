@Article{Martinez1995,
  Title                    = {Considerations on the use of gill nets in community structure studies of fishes in Cuba},
  Author                   = {Martinez, A.P. and Escobar, R.D. and Prokes, M. and Penaz, M.},
  Journal                  = {Folia Zool},
  Pages                    = {263--268},
  Volume                   = {44},
  Year   umber                   = {3},

  Comment                  = {ISI DATA A sampling of the ichthyofauna associated with mangrove was carried out from the 6th to the 10th of March, 1989, in the Ensenada de Majana, Cuba, with the aim of evaluating the advantages and limitations of the use of gill nets in community structure studies of fishes. For sampling, 13 gill nets of mesh size 12 - 70 mm (non- stretched) were laid along the mangrove's shoreline. It was found that nocturnal captures were higher than diurnal, not only in number of species, but in values of diversity and equitability. At the same time, an inventory of the ichthyofauna present under the mangrove was accomplished, using sub-aquatic transects. The data obtained were similar to that of diurnal species captured in gill nets. From these results we infer that the change in composition of nocturnal captures with gal nets shows the re- distribution of the species in the mangrove caused by changes in illumination levels. This factor is rarely taken into account when studies of the community structure are based only on diurnal sub-aquatic inventories Article},
  Keywords                 = {COMMUNITY STRUCTURE, Cuba, sampling, ichthyofauna, MANGROVES, DIVERSITY, inventory, distribution},
  Owner                    = {aellison},
  Refid                    = {7407},
  Timestamp                = {2013.11.20}
