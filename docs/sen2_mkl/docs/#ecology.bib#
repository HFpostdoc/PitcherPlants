@Article{Momonoki1996,Salicornia europaea L. III. Salt accumulation and ACh function},
  Author                   = {Momonoki, Y.S. and Oguri, S. and Kato, S. and Kamimura, H.},
  Journal                  = {Japanese Journal of Crop Science},
