@Article{Molina-Lara1995,
  Title                    = {Polychaetes (Annelida-Polychaeta) del estero de Jaltepeque, El Salvador: una comparaci¢n 1959-1991},
  Author                   = {Molina-Lara, O.A. and Vargas-Zamora, J.A.},
  Journal                  = {Rev.Biol.Trop},
  Pages                    = {195--205},
  Volume                   = {43},
  Year                     = {1995},
  Number                   = {1-3},

  Comment                  = {ISI DATA A survey of the intertidal polychaete fauna of the Jaltepeque estuary, Pacific coast of El Salvador (13 degrees N - 89 degrees W), was conducted during December of 1990 (dry season) and July of 1991 (rainy season). A total of 15 stations were sampled along the estuary, and 15 core samples (core area: 17.7 cm(2)) collected at each one, to a depth of 15 cm into the sediment. Samples were preserved in 10% formalin in sea water stained with Pose Bengal. A 500 micron mesh sieve was used to separate the organisms from the sediments. A total of 450 core samples yielded 5315 polychaetes distributed among 30 species. Densities ranged between 111 and 22 296 per m(2), with a mean of about 6 500. The salinity gradient along the axis of the estuary ranged between 14 and 30 parts per thousand during the rainy season, and from 22 to 30 parts per thousand during the dry season. This gradient is smaller than that observed (0 to 31 parts per thousand) in a polychaete survey conducted over, a one year period, at the site in the late 1950's. A comparison of the polychaete species found in both studies only yielded 28% similarity (Sorensen's index), even though samples were collected at nearly the same stations. Although the durations of both studies differ considerably, we expected to find more species in common. The total number of species collected in both studies was, however, very similar (30 and 36). Data presented in this paper provide evidence to support the hypothesis that the polychaete fauna in the Jaltepeque estuary is presently composed of more eurihaline species than in the past. The Jaltepeque estuary region has undergone considerable development over recent decades, but a direct impact on its benthic fauna is difficult to assess. That this fauna was studied in the late 1950's as well as in this paper, provides a unique reference framework, very rare in benthic tropical research, for future more complex surveys Article},
  Keywords                 = {polychaetes, El Salvador, SURVEY, intertidal ecology, estuaries, Pacific Ocean, sediment, SALINITY, GRADIENTS, similarity, development, RARITY, tropical, research, tropical benthos, MANGROVES, MACROFAUNA, benthic communities},
  Owner                    = {aellison},
  Refid                    = {7396},
  Timestamp                = {2013.11.20}
