@Article{Sheridan1997,
  Title                    = {Benthos of adjacent mangrove, seagrass and non-vegetated habitats in Rookery Bay, Florida, USA},
  Author                   = {Sheridan, P.},
  Journal                  = {Estuarine, Coastal and Shelf Science},
  Pages                    = {455--469},
  Volume                   = {44},
