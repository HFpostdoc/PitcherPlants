@Article{Gwada2000,
  Title                    = {Leaf phenological traits in the mangrove Kandelia candel (L.) Druce},
  Author                   = {Gwada, P. and Makoto, T. and Uezu, Y.},
  Journal                  = {Aquatic Botany},
  Pages                    = {1--14},
